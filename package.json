{
  "name": "ws-nats",
  "version": "1.0.0",
  "description": "WebSocket NATS client",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "test": "tsc && nyc ava --verbose -T 15000",
    "coveralls": "nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "cover": "nyc report --reporter=html && open coverage/index.html",
    "debugtest": "tsc && node $NODE_DEBUG_OPTION node_modules/.bin/ava --verbose -T 6500000 -m",
    "package": "webpack-cli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//aricart/ws-nats"
  },
  "keywords": [
    "NATS",
    "websockets"
  ],
  "author": "Alberto Ricart",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^10.3.1",
    "@types/ws": "^5.1.1",
    "ava": "1.0.0-beta.4",
    "awesome-typescript-loader": "^5.0.0",
    "browser-env": "^3.2.5",
    "coveralls": "^3.0.1",
    "fast-text-encoding": "^1.0.0",
    "jsdom": "^11.11.0",
    "nyc": "^11.9.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.3.1",
    "typescript": "^2.9.1",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.5",
    "ws": "latest"
  },
  "ava": {
    "require": [
      "./lib/test/helpers/setup-browser-env.js"
    ],
    "files": [
      "./lib/test/**/*.js",
      "!./lib/test/helpers/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "js-nuid": "^1.0.1"
  }
}
